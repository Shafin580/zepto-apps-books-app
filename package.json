{
  "name": "next.js-base-multimodule",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=6144' rm -rf .next && next dev",
    "devHttps": "rm -rf .next && next dev --experimental-https",
    "build": "rm -rf .next && NODE_ENV=production next build",
    "build-test": "rm -rf .next && NODE_ENV=test next build",
    "clean-install": "rm -rf node_modules pnpm-lock.yaml yarn.lock bun.lockb package-lock.json && pnpm install",
    "clean-install-pmy": "rm -rf node_modules pnpm-lock.yaml yarn.lock bun.lockb package-lock.json && yarn",
    "clean-install-rb": "rm -rf node_modules pnpm-lock.yaml yarn.lock bun.lockb package-lock.json && bun install",
    "clean-build": "pnpm run clean-install && pnpm build",
    "clean-build-pmy": "yarn run clean-install-pmy && yarn run build",
    "clean-build-rb": "bun run clean-install-rb && bun run build",
    "start": "NODE_ENV=production next start -p 8080",
    "start-local": "NODE_ENV=production next start -p 3000",
    "start-test-local": "NODE_ENV=test next start -p 3000",
    "pre-commit": "lint-staged --allow-empty",
    "commit": "npx cz",
    "format": "prettier --write .",
    "lint": "next lint",
    "prepare": "husky install",
    "update-icon-names": "node ./app/components/library/Icon/icon-name-generator.mjs",
    "analyze": "ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "push": "git push --no-verify"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs,css,scss,json,html}": [
      "prettier --write"
    ]
  },
